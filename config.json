{
  "offsetX": 40,
  "offsetY": 40,
  "width": 800,
  "height": 64,
  "startSelected": true,
  "intervalMinutes": 30,
  "hotKey": "cmd+shift+=",
  "closeOnBlur": true,
  "showOnStart": true,
  "showDockIcon": false,
  "showMenuBarIcon": true,
  "theme": {
    "backgroundColor": "#333333F2",
    "borderColor": "#666666AA",
    "borderWidth": 1.0,
    "cornerRadius": 12,
    "textColor": "#FFFFFF",
    "placeholderColor": "#BBBBBB99",
    "fontSize": 18
  },
  "filesDefault": {
    "insertBehaviour": "date",
    "file": "/Users/yuri/Obsidian/WIP.md"
  },
  "files": {
    "1": {
      "promptMatch": ".*#task.*",
      "filePositionMatch": "^- \\[",
      "insertBehavior": "endoflist",
      "exitBehaviour": "finish",
      "file": "/Users/yuri/Obsidian/üìù Todo.md",
      "format": "- [ ] $prompt"
    },
    "2": {
      "promptMatch": ".*",
      "directory": "/Users/yuri/Obsidian/Global/Dailies",
      "fileMatch": "^\\d{4}-\\d{2}-\\d{2}.*\\.md$",
      "filePositionMatch": "(?m)^## üìù Tasks$",
      "insertBehavior": "endoflist",
      "exitBehaviour": "finish",
      "format": "- [ ] $prompt"
    }
  }
}
